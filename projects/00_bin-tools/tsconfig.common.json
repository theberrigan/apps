{
    "compileOnSave": false,
    "compilerOptions": {
        "lib": [
            "dom",
            "dom.iterable",
            "es2015",
            "es2016",
            "es2017",
            "es2018",
            "es2019",
            "es2020",
            "es2021",
            "es2022",
            "es2023",
            "es2024",
            "esNext",
        ],
        "target": "es2022",
        "module": "commonjs",
        "moduleResolution": "node",
        "allowJs": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitOverride": true,
        "noImplicitReturns": true,
        "noPropertyAccessFromIndexSignature": true,
        "strictNullChecks": false,
        "stripInternal": false,
        "sourceMap": true,
        "strict": true
    }
}
