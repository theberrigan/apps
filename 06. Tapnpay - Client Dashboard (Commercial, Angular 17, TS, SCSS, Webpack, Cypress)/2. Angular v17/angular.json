{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "tnp-dashboard": {
            "projectType": "application",
            "schematics": {},
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "main": "src/main.ts",
                        "polyfills": [
                            "zone.js"
                        ],
                        "tsConfig": "tsconfig.app.json",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "index": "src/index.html",
                        "styles": [
                            "src/assets/css/styles.scss"
                        ],
                        "scripts": [],
                        "allowedCommonJsDependencies": [
                            "@messageformat/core",
                            "flatpickr",
                            "braintree-web",
                            "moment",
                            "simplebar",
                            "core-js",
                            "crypto-js",
                            "lodash",
                            "util",
                            "copy-to-clipboard",
                            "js-cookie",
                            "scrollbarwidth",
                            "src/app/modules/app/app.component.ts"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "config/app/dev.js",
                                    "with": "config/app/prod.js"
                                }
                            ],
                            "outputPath": "dist/prod",
                            "optimization": true,
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "subresourceIntegrity": false,
                            "deleteOutputPath": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "350kb",
                                    "maximumError": "500kb"
                                }
                            ]
                        },
                        "staging": {
                            "fileReplacements": [
                                {
                                    "replace": "config/app/dev.js",
                                    "with": "config/app/stage.js"
                                }
                            ],
                            "outputPath": "dist/stage",
                            "optimization": true,
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "subresourceIntegrity": false,
                            "deleteOutputPath": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "350kb",
                                    "maximumError": "500kb"
                                }
                            ]
                        },
                        "dev": {
                            "outputPath": "dist/dev",
                            "optimization": true,
                            "sourceMap": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "subresourceIntegrity": false,
                            "deleteOutputPath": true
                        },
                        "demo": {
                            "fileReplacements": [
                                {
                                    "replace": "config/app/dev.js",
                                    "with": "config/app/demo.js"
                                }
                            ],
                            "outputPath": "dist/demo",
                            "optimization": true,
                            "sourceMap": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "subresourceIntegrity": false,
                            "deleteOutputPath": true
                        },
                        "mock": {
                            "fileReplacements": [
                                {
                                    "replace": "config/app/dev.js",
                                    "with": "config/app/mock.js"
                                }
                            ],
                            "outputPath": "dist/mock",
                            "optimization": true,
                            "sourceMap": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "subresourceIntegrity": false,
                            "deleteOutputPath": true
                        },
                        "development": {
                            "outputPath": "dist/dev",
                            "buildOptimizer": false,
                            "optimization": false,
                            "sourceMap": true,
                            "namedChunks": true,
                            "extractLicenses": false,
                            "vendorChunk": true,
                            "subresourceIntegrity": false,
                            "deleteOutputPath": true
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "host": "127.0.0.1",
                        "port": 15099,
                        "ssl": false,
                        "sslKey": "misc/cert/server.key",
                        "sslCert": "misc/cert/cert.pem",
                        "buildTarget": "tnp-dashboard:build"
                    },
                    "configurations": {
                        "development": {
                            "buildTarget": "tnp-dashboard:build:development"
                        },
                        "production": {
                            "buildTarget": "tnp-dashboard:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "buildTarget": "tnp-dashboard:build"
                    }
                },
                "e2e": {
                    "builder": "@cypress/schematic:cypress",
                    "options": {
                        "devServerTarget": "tnp-dashboard:serve",
                        "watch": true,
                        "headless": false
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "tnp-dashboard:serve:production"
                        }
                    }
                },
                "cypress-run": {
                    "builder": "@cypress/schematic:cypress",
                    "options": {
                        "devServerTarget": "tnp-dashboard:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "tnp-dashboard:serve:production"
                        }
                    }
                },
                "cypress-open": {
                    "builder": "@cypress/schematic:cypress",
                    "options": {
                        "watch": true,
                        "headless": false
                    }
                },
                "ct": {
                    "builder": "@cypress/schematic:cypress",
                    "options": {
                        "devServerTarget": "tnp-dashboard:serve",
                        "watch": true,
                        "headless": false,
                        "testingType": "component"
                    },
                    "configurations": {
                        "development": {
                            "devServerTarget": "tnp-dashboard:serve:development"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "src/**/*.ts",
                            "src/**/*.html"
                        ]
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:jest",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ],
                        "tsConfig": "tsconfig.spec.json"
                    }
                }
            }
        }
    },
    "cli": {
        "schematicCollections": [
            "@angular-eslint/schematics"
        ]
    }
}
